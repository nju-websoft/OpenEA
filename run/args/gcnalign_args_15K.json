{
	"training_data": "../../datasets/",
  	"output": "../../output/results/",
  	"dataset_division": "721_5fold",

  	"embedding_module": "GCN_Align",
  	"alignment_module": "mapping",
	"search_module": "greedy",

    "dim": 100,

	"neg_sampling": "uniform",
	"neg_triple_num": 5,

	"learning_rate": 8,
	"max_epoch": 2000,
	"batch_size": 5000,

	"batch_threads_num": 2,
	"test_threads_num": 3,

	"ordered": true,
	"start_valid": 100,
	"eval_freq": 10,
	"stop_metric": "hits1",
	"eval_metric": "manhattan",
	"csls": 10,
	"top_k": [1, 5, 10, 50],
	"is_save": true,
	"eval_norm": false,

    "support_number": 1,
    "se_dim": 100,
    "ae_dim": 100,
    "hidden1": 100,
    "gamma": 3,
    "early_stop": false,
    "dropout": 0,

	"test_method": "sa",
	"beta": 0.9
